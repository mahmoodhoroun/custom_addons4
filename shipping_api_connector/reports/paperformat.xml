<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Custom paper format for Barid.ma shipping label (104mm x 104mm) -->
    <record id="paperformat_barid_ma_label" model="report.paperformat">
        <field name="name">Barid.ma Label</field>
        <field name="default" eval="False"/>
        <field name="format">custom</field>
        <field name="page_width">104</field>
        <field name="page_height">104</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">2</field>
        <field name="margin_right">2</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>
</odoo>
